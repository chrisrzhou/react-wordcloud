{
  "name": "react-wordcloud",
  "version": "1.0.0",
  "description": "React + D3 word cloud component with rich features. Based on the original word cloud generator: https://www.jasondavies.com/wordcloud/.",
  "main": "dist/index.js",
  "module": "dist/index.module.js",
  "keywords": [
    "react",
    "d3",
    "wordcloud"
  ],
  "author": "Chris Zhou <christoper.r.zhou@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/chrisrzhou/react-wordcloud.git"
  },
  "bugs": {
    "url": "https://github.com/chrisrzhou/react-wordcloud/issues"
  },
  "homepage": "https://wordcloud.chrisrzhou.io",
  "scripts": {
    "prepare": "yarn clean && yarn build",
    "build": "rollup -c",
    "build:docs": "yarn docz build",
    "clean": "rm -rf dist .docz",
    "dev": "yarn docz dev",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx, --fix src",
    "size": "bundlesize"
  },
  "dependencies": {
    "@types/d3-cloud": "^1.2.1",
    "d3-cloud": "^1.2.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@types/react": "^16.8.3",
    "@typescript-eslint/eslint-plugin": "^1.3.0",
    "@typescript-eslint/parser": "^1.3.0",
    "bundlesize": "^0.17.0",
    "docz": "^0.13.7",
    "docz-theme-default": "^0.13.7",
    "eslint": "^5.10.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.12.4",
    "prettier": "^1.15.3",
    "react": "^16.8.1",
    "rollup": "^1.0.0",
    "rollup-plugin-babel": "^4.1.0",
    "typescript": "^3.3.3"
  },
  "bundlesize": [
    {
      "path": "./dist/*.js",
      "maxSize": "10 kB"
    }
  ],
  "peerDependencies": {
    "react": "^16.8.1"
  }
}
