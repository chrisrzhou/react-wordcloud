{
  "name": "react-wordcloud",
  "version": "1.0.7",
  "description": "Simple React + D3 wordcloud component with powerful features.",
  "main": "dist/index.js",
  "module": "dist/index.module.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "react",
    "d3",
    "wordcloud",
    "visualization"
  ],
  "author": "Chris Zhou <christoper.r.zhou@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/chrisrzhou/react-wordcloud.git"
  },
  "bugs": {
    "url": "https://github.com/chrisrzhou/react-wordcloud/issues"
  },
  "homepage": "https://react-wordcloud.netlify.com/",
  "scripts": {
    "build": "rollup -c",
    "build:docs": "yarn docz build",
    "clean": "rm -rf dist .docz",
    "dev": "yarn docz dev",
    "lint": "yarn tsc && eslint --ext .js,.jsx,.ts,.tsx --fix src docs",
    "prepare": "yarn clean && yarn build",
    "size": "bundlesize"
  },
  "dependencies": {
    "@types/d3-array": "^2.0.0",
    "@types/d3-cloud": "^1.2.2",
    "@types/d3-scale": "^2.1.1",
    "@types/d3-scale-chromatic": "^1.3.1",
    "@types/d3-selection": "^1.4.1",
    "@types/d3-transition": "^1.1.4",
    "@types/seedrandom": "^2.4.28",
    "d3-array": "^2.2.0",
    "d3-cloud": "^1.2.5",
    "d3-scale": "^3.0.0",
    "d3-scale-chromatic": "^1.3.3",
    "d3-selection": "^1.4.0",
    "d3-transition": "^1.2.0",
    "resize-observer-polyfill": "^1.5.1",
    "seedrandom": "^3.0.1",
    "tippy.js": "^4.3.4"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@types/react": "^16.8.8",
    "@typescript-eslint/eslint-plugin": "^1.4.2",
    "@typescript-eslint/parser": "^1.4.2",
    "bundlesize": "^0.17.0",
    "docz": "^1.2.0",
    "docz-theme-default": "^1.2.0",
    "eslint": "^5.15.2",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.5.1",
    "prettier": "^1.15.3",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "rollup": "^1.6.0",
    "rollup-plugin-babel": "^4.1.0",
    "rollup-plugin-typescript2": "^0.21.1",
    "tslib": "*",
    "typescript": "^3.3.3333"
  },
  "bundlesize": [
    {
      "path": "./dist/*.js",
      "maxSize": "10 kB"
    }
  ],
  "peerDependencies": {
    "react": "^16.8.3"
  }
}
