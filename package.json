{
  "name": "react-wordcloud",
  "version": "1.2.0",
  "description": "Simple React + D3 wordcloud component with powerful features.",
  "main": "dist/index.js",
  "module": "dist/index.module.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "react",
    "d3",
    "wordcloud",
    "visualization"
  ],
  "author": "Chris Zhou <chrisrzhou@pm.me>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/chrisrzhou/react-wordcloud.git"
  },
  "bugs": {
    "url": "https://github.com/chrisrzhou/react-wordcloud/issues"
  },
  "homepage": "https://react-wordcloud.netlify.com/",
  "scripts": {
    "build": "rollup -c",
    "build:docs": "yarn clean:docs && yarn docz build",
    "clean": "rm -rf dist",
    "clean:docs": "rm -rf .docz",
    "dev": "yarn clean:docs && yarn docz dev",
    "lint": "yarn xo",
    "prepare": "yarn clean && yarn build",
    "size": "bundlesize",
    "unused": "yarn no-unused-export src"
  },
  "dependencies": {
    "@types/d3-array": "^2.0.0",
    "@types/d3-cloud": "^1.2.2",
    "@types/d3-dispatch": "^1.0.7",
    "@types/d3-scale": "^2.2.0",
    "@types/d3-scale-chromatic": "^1.3.1",
    "@types/d3-selection": "^1.4.1",
    "@types/d3-transition": "^1.1.6",
    "@types/lodash.clonedeep": "^4.5.6",
    "@types/lodash.debounce": "^4.0.6",
    "@types/seedrandom": "^2.4.28",
    "d3-array": "^2.4.0",
    "d3-cloud": "^1.2.5",
    "d3-dispatch": "^1.0.6",
    "d3-scale": "^3.2.1",
    "d3-scale-chromatic": "^1.5.0",
    "d3-selection": "1.4.1",
    "d3-transition": "^1.3.2",
    "lodash.clonedeep": "^4.5.0",
    "lodash.debounce": "^4.0.8",
    "resize-observer-polyfill": "^1.5.1",
    "seedrandom": "^3.0.5",
    "tippy.js": "^6.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@types/react": "^16.9.23",
    "@typescript-eslint/eslint-plugin": "^2.22.0",
    "@typescript-eslint/parser": "^2.22.0",
    "bundlesize": "^0.18.0",
    "docz": "^2.2.0",
    "docz-theme-default": "^1.2.0",
    "eslint": "^6.8.0",
    "eslint-config-xo-react": "^0.23.0",
    "no-unused-export": "^1.10.1",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "rollup": "^2.0.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-typescript2": "^0.26.0",
    "tslib": "1.11.1",
    "typescript": "3.8.3",
    "xo": "^0.27.2"
  },
  "bundlesize": [
    {
      "path": "./dist/*.js",
      "maxSize": "10 kB"
    }
  ],
  "peerDependencies": {
    "react": "^16.13.0"
  },
  "resolutions": {
    "d3-selection": "1.4.1"
  },
  "xo": {
    "prettier": {
      "trailingComma": "always"
    }
  }
}
